    "compodoc": "./node_modules/.bin/compodoc my-lib -p ./tsconfig.json"
  },
  "dependencies": {
    "@angular/cdk": "^5.1.0",
    "@angular/common": "~5.2.0",
    "@angular/compiler": "~5.2.0",
    "@angular/core": "~5.2.0",
    "@angular/forms": "~5.2.0",
    "@angular/http": "~5.2.0",
    "@angular/platform-browser": "~5.2.0",
    "@angular/platform-browser-dynamic": "~5.2.0",
    "@angular/router": "~5.2.0",
    "@compodoc/compodoc": "^1.0.5",
    "core-js": "^2.4.1",
    "meepo-utils": "^1.0.0-alpha.0",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^0.0.48",
    "@angular/cli": "~1.6.0",
