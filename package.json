    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "2.0.0-rc.10",
    "protractor": "~5.2.0",
    "rimraf": "^2.6.1",
    "ts-node": "~4.1.0",
    "tsickle": "^0.26.0",
    "tslint": "~5.9.0",
    "typescript": "~2.6.1"
